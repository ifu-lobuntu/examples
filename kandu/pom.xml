<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.jbpm</groupId>
    <artifactId>examples</artifactId>
    <version>1.0</version>
  </parent>
  <groupId>org.jbpm</groupId>
  <artifactId>kandu</artifactId>
  <version>1.0</version>
  <name>kandu</name>
  <description>Kandu is a project that aims to co-ordinate exchanges between consumers, multiple informal farmers and supporting participants such as transport providers, hub operators and training staff</description>
  <build>
    <plugins>
      <plugin>
        <groupId>org.kie</groupId>
        <artifactId>kie-maven-plugin</artifactId>
        <version>6.3.0-SNAPSHOT</version>
        <extensions>true</extensions>
      </plugin>
    </plugins>
  </build>
</project>
